{
	"name": "manager_trigger",
	"properties": {
		"description": "manager drop any kind of file in source folder.",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "ManagerPipeline",
					"type": "PipelineReference"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/source/blobs/",
			"blobPathEndsWith": ".csv",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/13cb5a24-e2b6-4b34-a645-b5ca81b491fc/resourceGroups/rg-sales/providers/Microsoft.Storage/storageAccounts/salsestorage",
			"events": [
				"Microsoft.Storage.BlobCreated",
				"Microsoft.Storage.BlobDeleted"
			]
		}
	}
}