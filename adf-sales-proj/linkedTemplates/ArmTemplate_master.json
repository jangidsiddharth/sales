{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-sales-proj"
		},
		"managerLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'managerLinkedService'"
		},
		"metadata_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'metadata_linked_service'"
		},
		"managerLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://salsestorage.dfs.core.windows.net/"
		},
		"metadata_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://salsestorage.dfs.core.windows.net/"
		},
		"manager_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/13cb5a24-e2b6-4b34-a645-b5ca81b491fc/resourceGroups/rg-sales/providers/Microsoft.Storage/storageAccounts/salsestorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-sales-proj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"managerLinkedService_accountKey": {
						"value": "[parameters('managerLinkedService_accountKey')]"
					},
					"metadata_linked_service_accountKey": {
						"value": "[parameters('metadata_linked_service_accountKey')]"
					},
					"managerLinkedService_properties_typeProperties_url": {
						"value": "[parameters('managerLinkedService_properties_typeProperties_url')]"
					},
					"metadata_linked_service_properties_typeProperties_url": {
						"value": "[parameters('metadata_linked_service_properties_typeProperties_url')]"
					},
					"manager_trigger_properties_typeProperties_scope": {
						"value": "[parameters('manager_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}